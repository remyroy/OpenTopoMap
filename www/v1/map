<html>
	<head>
	<head>
	<body>
		<script type="text/javascript">
			if(window.location.hash.split("|").length-1 == 2) {
				var position = window.location.hash.substr(1).split("|");
				if(isNaN(position[0]) || isNaN(position[1]) || isNaN(position[2])) {
					self.location.href="/";
				} else {
					self.location.href='/#map='+position[2]+'/'+position[0]+'/'+position[1];
				}
			} else {
				self.location.href="/";
			}
		</script>
	</body>
</html>
